<!-- index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
</head>
<body>
    <h1>User Data</h1>
    <table>
        <thead>
            <tr>
                <th>Email</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach((user) => { %>
                <tr>
                    <td><%= user.email %></td>
                    <td><%= user.pass %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
